{
  "name": "front-end-build-pack",
  "description": "This README would normally document whatever steps are necessary to get your application up and running.",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/joe_diton/buildsystem.git"
  },
  "keywords": [
    "gulp",
    "tailwind",
    "react",
    "postcss",
    "nunjucks",
    "webpack"
  ],
  "author": "Joe Diton",
  "license": "ISC",
  "homepage": "https://bitbucket.org/joe_diton/buildsystem#readme",
  "main": "gulpfile.js",
  "paths": {
    "build": "./_build/",
    "css": {
      "src": "./_src/css/**/",
      "dest": "./_build/_assets/css/"
    },
    "html": {
      "src": "./_src/html/**/",
      "dest": "./_build/"
    },
    "js": {
      "src": "./_src/js/**/",
      "dest": "./_build/_assets/js/"
    }
  },
  "browserSync": {
    "open": false,
    "server": {
      "baseDir": "./_build"
    }
  },
  "scripts": {
    "start": "gulp --maps",
    "build": "gulp build --minify"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "browser-sync": "^2.26.7",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-nunjucks": "^5.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-purgecss": "^1.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-util": "^3.0.8",
    "postcss-import": "^12.0.1",
    "postcss-scss": "^2.0.0",
    "precss": "^4.0.0",
    "tailwindcss": "^1.0.3"
  }
}
